{
  presets: [
    'env',
    'latest',
    'react',
    'stage-2'
  ],
  comments: false,
  plugins: [
    [
      'import',
      {
        libraryName: 'antd',
        libraryDirectory: 'lib',
        style: 'css'
      },
    ],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  env: {
    test: {
      plugins: [
        'istanbul'
      ]
    }
  }
}
